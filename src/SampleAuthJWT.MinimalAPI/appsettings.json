{
    "ConnectionStrings": {
        "SqlConnection": "Data Source=[HOSTNAME];Initial Catalog=[DATABASE];User ID=[USERNAME];Password=[PASSWORD];Encrypt=False"
    },
    "Authentication": {
        "DefaultScheme": "Bearer",
        "JwtBearer": {
            "SchemeName": "Bearer",
            "SecurityKey": "",
            "Issuers": [ "sampleauthjwt" ],
            "Audiences": [ "sampleauthjwt" ],
            "ExpirationTime": "01:00:00",
            "ClockSkew": "00:02:00",
            "EnableJwtBearerService": true
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}